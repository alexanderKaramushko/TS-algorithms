{
  "version": "0.1.0",
  "configurations": [
    {
      "name": "Debugger",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "node",
      "cwd": "${workspaceFolder}/src",
      "runtimeArgs": [
        "--nolazy",
        "-r",
        "ts-node/register/transpile-only"
      ],
      "args": [
        "index.ts"
      ],
      "skipFiles": [
        "<node_internals>/**",
        "node_modules/**"
      ]
    }
  ]
}